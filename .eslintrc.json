{
  "env": {
      "browser": true,
      "es6": true
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "jsx-one-expression-per-line": 0,
      "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": 0,
      "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ]
}
